{
  "compilerOptions": {
    "outDir": "dist",
    "target": "ESNext",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "skipLibCheck": true,
    /* Bundler mode */
    "jsx": "react-jsx",
    "declaration": true,
    /* Linting */
    "strict": true,
  },
  "include": ["src/**/*"],
}